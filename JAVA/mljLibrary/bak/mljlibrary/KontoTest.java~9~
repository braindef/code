import Konto.Konto;

public class KontoTest {
  public static void main(String[] args) {
    Konto kto = new Konto();
    kto.setzeZinsfuss(2.25);
    kto.einzahlung(1, 4);
    kto.getSaldo();
    kto.addiereJahreszins();
  }
}